pipeline {
	agent any
		steps {
		stage('build images') {
			steps {
				sh "./scripts/build_images.sh"
			}
		}
		stage('test-app') {
                        steps {
                                sh "./scripts/run.sh"
			}
		}
		stage('push_images') {
			steps {
				sh "./scripts/push_images.sh"
			}
		}
	}
}
